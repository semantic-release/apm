{
  "name": "@semantic-release/apm",
  "description": "semantic-release plugin to publish Atom packages with apm",
  "version": "0.0.0-development",
  "author": "Pierre Vanduynslager (https://twitter.com/@pvdlg_)",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "helpers": [
      "test/helpers/**/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/semantic-release/apm/issues"
  },
  "contributors": [
    "Stephan BÃ¶nnemann <stephan@boennemann.me> (http://boennemann.me)",
    "Gregor Martynus (https://twitter.com/gr2m)"
  ],
  "dependencies": {
    "@semantic-release/error": "^2.2.0",
    "aggregate-error": "^3.0.0",
    "execa": "^3.2.0",
    "npm": "^6.4.1",
    "read-pkg": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^2.0.0",
    "clear-module": "^4.0.0",
    "codecov": "^3.1.0",
    "dockerode": "^3.0.0",
    "fs-extra": "^8.0.0",
    "get-stream": "^5.0.0",
    "got": "^10.2.0",
    "mockserver-client": "^5.5.0",
    "nyc": "^15.0.0",
    "p-retry": "^4.0.0",
    "semantic-release": "^15.12.3",
    "sinon": "^8.0.0",
    "stream-buffers": "^3.0.2",
    "tempy": "^0.3.0",
    "xo": "^0.25.0"
  },
  "engines": {
    "node": ">=8.16"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "homepage": "https://github.com/semantic-release/apm#readme",
  "keywords": [
    "apm",
    "atom",
    "atom-packages",
    "publish",
    "registry",
    "semantic-release",
    "version"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "peerDependencies": {
    "semantic-release": ">=15.9.0 <16.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semantic-release/apm.git"
  },
  "scripts": {
    "codecov": "codecov -f coverage/coverage-final.json",
    "lint": "xo",
    "pretest": "npm run lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v"
  },
  "xo": {
    "prettier": true,
    "space": true
  }
}
